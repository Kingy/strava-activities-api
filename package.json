{
  "name": "strava-activities-backend",
  "version": "1.0.0",
  "description": "Backend API for Strava Activities World Map",
  "main": "lambda.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "create-tables": "node scripts/create-tables.js",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "offline": "serverless offline",
    "logs": "serverless logs -f api -t",
    "remove": "serverless remove",
    "test:local": "serverless invoke local -f api",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "strava",
    "activities",
    "maps",
    "express",
    "oauth"
  ],
  "author": "Jamie Gracie",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.840.0",
    "@aws-sdk/lib-dynamodb": "^3.840.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "serverless-domain-manager": "^8.0.0",
    "serverless-offline": "^14.4.0"
  }
}
